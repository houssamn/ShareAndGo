<?xml version="1.0" encoding="UTF-8"?>

<ns1:window-layout
    ns1:id="Home"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://actiongui.org/schemes/ag-layout/2.4'
    xsi:schemaLocation='http://actiongui.org/schemes/ag-layout/2.4 http://actiongui.org/schemes/ag-layout/2.4/schema.xsd'>
    
    <ns1:line-layout orientation="horizontal" ns1:height="100%">
        <ns1:entry expand-ratio="1">
            <ns1:line-layout orientation="horizontal"/>
        </ns1:entry>
        <ns1:entry>
            <ns1:line-layout orientation="vertical" ns1:height="100%" ns1:width="500px">
                <ns1:entry expand-ratio="1">
                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
                <ns1:entry>
                    <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="100%">
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                        <ns1:entry>
                            <ns1:line-layout orientation="vertical" ns1:height="auto" ns1:width="100%">
                                <ns1:entry>
                                    <ns1:widget ns1:id="userInfo_L" ns1:width="100%" ns1:height="23px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:table ns1:id="userInfo" ns1:width="100%" ns1:height="100px">
                                        <ns1:column>
                                            <ns1:widget ns1:id="zone" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                        <ns1:column>
                                            <ns1:widget ns1:id="username" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                        <ns1:column>
                                            <ns1:widget ns1:id="fName" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                        <ns1:column>
                                            <ns1:widget ns1:id="lastName" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                    </ns1:table>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="allUsers_L" ns1:width="100%" ns1:height="23px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:table ns1:id="allUsers" ns1:width="100%" ns1:height="100px">
                                        <ns1:column>
                                            <ns1:widget ns1:id="fName" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                        <ns1:column>
                                            <ns1:widget ns1:id="lastName" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                    </ns1:table>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="allTrips_L" ns1:width="100%" ns1:height="23px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:table ns1:id="allTrips" ns1:width="100%" ns1:height="100px">
                                        <ns1:column>
                                            <ns1:widget ns1:id="driver" ns1:width="100%" ns1:height="23px"/>
                                        </ns1:column>
                                    </ns1:table>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="CreateTrip_B" ns1:width="100%" ns1:height="auto"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:widget ns1:id="Report_B" ns1:width="100%" ns1:height="auto"/>
                                </ns1:entry>
                                <ns1:entry>
                                    <ns1:line-layout orientation="horizontal" ns1:height="10px"/>
                                </ns1:entry>
                            </ns1:line-layout>
                        </ns1:entry>
                        <ns1:entry expand-ratio="1">
                            <ns1:line-layout orientation="horizontal"/>
                        </ns1:entry>
                    </ns1:line-layout>
                </ns1:entry>
                <ns1:entry expand-ratio="1">
                    <ns1:line-layout orientation="horizontal"/>
                </ns1:entry>
            </ns1:line-layout>
        </ns1:entry>
        <ns1:entry expand-ratio="1">
            <ns1:line-layout orientation="horizontal"/>
        </ns1:entry>
    </ns1:line-layout>

</ns1:window-layout>

